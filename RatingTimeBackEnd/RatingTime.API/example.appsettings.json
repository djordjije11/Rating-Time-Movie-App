{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "LogFile": {
      "Include": false,
      "Path": "Logs\\log-{Date}.txt"
    }
  },
  "AllowedHosts": "*",
  "CORS": {
    "Origins": [ "http://localhost:3000", "http://127.0.0.1:3000", "http://172.25.192.1:3000", "http://192.168.0.22:3000" ]
  },
  "Provider": "SqlServer",
  "ConnectionStrings": {
    "SqlServer": "{connection_string_of_your_database}",
    "MySql": "Server=localhost; User ID=root; Password=admin; Database=rating_time_schema"
  },
  "Seeding": {
    "SeedData": true,
    "TmdbApiKey": "{tmdb_api_key}"
  }
}