{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "LogFile": {
      "Include": false,
      "Path": "Logs\\log-{Date}.txt"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Default": "{connection_string_of_your_database}"
  }
}